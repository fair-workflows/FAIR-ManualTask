  sub:step1 dc:description "When mice have reached their endpoint, euthanize them with a lethal injection of pentobarbital (i.p. 10 mg/kg).";
    p-plan:hasInputVar sub:sample;
    p-plan:hasOutputVar sub:euthanized_mice;
